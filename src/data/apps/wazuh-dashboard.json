    {
      "label": "Get version",
      "value": "bin/openserach-dashboards --version"
    },
    {
      "label": "Plugins - Install by URL",
      "value": "bin/openserach-dashboards-plugin install <url>"
    },
    {
      "label": "Plugins - Install local file",
      "value": "bin/openserach-dashboards-plugin install file://<absolute_path_to_local_file>"
    },
    {
      "label": "Plugins - Remove",
      "value": "bin/openserach-dashboards-plugin remove <plugin_name>"
    },
    {
      "label": "Plugins - List",
      "value": "bin/openserach-dashboards-plugin list"
    }
  ],
  "debug": [
    {
      "label": "Logs (journalctl) - get all logs",
      "value": "journalctl -u wazuh-dashboard"
    },
    {
      "label": "Logs (journalctl) - get logs filtering by errors or warnings",
      "value": "journalctl -u wazuh-dashboard | grep -i -E \"err|warn\""
